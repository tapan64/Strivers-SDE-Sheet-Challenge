def getInOrderTraversal(root):
    res = []
    stk = deque()
    while True:
        if root:
            stk.append(root)
            root = root.left
        else:
            if stk:
                root = stk.pop()
                res.append(root.data)
                root = root.right
            else:
                break
    return res
