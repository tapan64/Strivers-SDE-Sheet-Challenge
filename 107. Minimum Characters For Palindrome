def isPalindrome(s, i, j):
    while i < j:
        if s[i] != s[j]:
            return False
        i += 1
        j -= 1
    return True

def minCharsforPalindrome(s):
    i, n = 0, len(s)
    for j in range(n - 1, -1, -1):
        if isPalindrome(s, i, j):
            return n - j - 1
    return 0
